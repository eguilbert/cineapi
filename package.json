{
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev --name update",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:sync": "npm run prisma:generate && npm run prisma:push",
    "postinstall": "prisma generate",
    "prestart": "prisma generate"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.15.0",
    "@supabase/supabase-js": "^2.50.5",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "h3": "^1.15.3",
    "jsonwebtoken": "^9.0.2",
    "lucia": "^3.2.2",
    "node-cron": "^4.2.1",
    "oslo": "^1.2.1",
    "prisma": "^5.22.0",
    "supabase": "^2.31.8"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
